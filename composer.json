{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "nikic/php-parser": "^4.3",
        "symfony/console": "5.0.*",
        "symfony/dependency-injection": "^5.0",
        "symfony/dotenv": "5.0.*",
        "symfony/finder": "^5.0",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/http-kernel": "^5.0",
        "symfony/yaml": "5.0.*",
        "symplify/autowire-array-parameter": "^7.2.8",
        "symplify/package-builder": "^7.2.8",
        "symplify/smart-file-system": "^7.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.3",
        "phpunit/phpunit": "^8.5",
        "rector/rector": "^0.7.7",
        "symplify/easy-coding-standard": "^7.2.8",
        "symplify/phpstan-extensions": "^7.2.8"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check src tests --ansi",
        "fix-cs": "vendor/bin/ecs check src tests --ansi --fix",
        "phpstan": "vendor/bin/phpstan analyse --ansi --error-format symplify"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
